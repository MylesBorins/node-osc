{
  "name": "node-osc",
  "description": "pyOSC inspired library",
  "version": "5.0.0",
  "main": "./dist/bundle.js",
  "exports": {
    "default": "./lib/index.mjs",
    "require": "./dist/bundle.js"
  },
  "author": {
    "name": "Myles Borins",
    "email": "myles.borins@gmail.com"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "LGPL-2.1",
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run build && npm run tap",
    "tap": "tap --no-esm --node-arg=--experimental-modules test/test-*{.js,.mjs}",
    "coverage": "npm run tap -- --coverage",
    "coverage-html": "npm run tap -- --coverage-report=html",
    "lint": "eslint lib/* test/* examples/*"
  },
  "contributors": [
    "Hans HÃ¼bner <hans.huebner@gmail.com>",
    "Andy Smith <jabber@term.ie>",
    "Myles Borins <myles.borins@gmail.com>"
  ],
  "keywords": [
    "osc",
    "udp"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/MylesBorins/node-osc.git"
  },
  "dependencies": {
    "jspack": "0.0.4",
    "osc-min": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "rollup": "^1.27.9",
    "tap": "^14.6.1"
  }
}
